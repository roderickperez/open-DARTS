[flake8]
max-line-length = 88
max-complexity = 10
exclude = 
    .git,
    __pycache__,
    .venv,
    venv,
    env,
    .env,
    build,
    dist,
    *.egg-info,
    .eggs,
    .tox,
    .pytest_cache,
    .mypy_cache,
    .coverage,
    htmlcov,
    .DS_Store,
    *.so,
    *.pyc,
    *.pyo,
    *.pyd,
    .Python,
    pip-log.txt,
    pip-delete-this-directory.txt,
    .tox,
    .coverage,
    .coverage.*,
    .cache,
    nosetests.xml,
    coverage.xml,
    *.cover,
    *.log,
    .git,
    .mypy_cache,
    .pytest_cache,
    .hypothesis,
    .venv,
    venv,
    env,
    .env,
    build,
    dist,
    *.egg-info,
    .eggs,
    .tox,
    .pytest_cache,
    .mypy_cache,
    .coverage,
    htmlcov,
    .DS_Store,
    *.so,
    *.pyc,
    *.pyo,
    *.pyd,
    .Python,
    pip-log.txt,
    pip-delete-this-directory.txt,
    .tox,
    .coverage,
    .coverage.*,
    .cache,
    nosetests.xml,
    coverage.xml,
    *.cover,
    *.log,
    .git,
    .mypy_cache,
    .pytest_cache,
    .hypothesis
# Ignored error codes:
# E203: whitespace before ':'
# E501: line too long (handled by black)
# W503: line break before binary operator
# W504: line break after binary operator
ignore = 
    E203,
    E501,
    W503,
    W504
per-file-ignores =
    __init__.py:F401 
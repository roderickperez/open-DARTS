{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "make release",
            "type": "shell",
            "command": "export PYTHON_IFLAGS=`python3-config --includes`;make -j 16",
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "presentation": {
                "echo": true,
                "reveal": "always",
                "focus": false,
                "panel": "shared"
            },
            "problemMatcher": {
                "base": "$gcc",
                "fileLocation": [
                    "relative",
                    "${workspaceRoot}"
                ]
            }
        },
        {
            "label": "make debug",
            "type": "shell",
            "command": "export PYTHON_IFLAGS=`python3-config --includes`;make debug -j 16",
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "presentation": {
                "echo": true,
                "reveal": "always",
                "focus": false,
                "panel": "shared"
            },
            "problemMatcher": {
                "base": "$gcc",
                "fileLocation": [
                    "relative",
                    "${workspaceRoot}"
                ]
            }
        },
        {
            "label": "make gpu release",
            "type": "shell",
            "command": "export PYTHON_IFLAGS=`python3-config --includes`;make gpu -j 6",
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "presentation": {
                "echo": true,
                "reveal": "always",
                "focus": false,
                "panel": "shared"
            },
            "problemMatcher": {
                "base": "$gcc",
                "fileLocation": [
                    "relative",
                    "${workspaceRoot}"
                ],
                "pattern": {
                    "regexp": "(.+)\\((\\d+)\\): (error|warning): (.+)",
                    "file": 1,
                    "line": 2,
                    "severity": 3,
                    "message": 4
                }
            }
        },
        {
            "label": "make gpu debug",
